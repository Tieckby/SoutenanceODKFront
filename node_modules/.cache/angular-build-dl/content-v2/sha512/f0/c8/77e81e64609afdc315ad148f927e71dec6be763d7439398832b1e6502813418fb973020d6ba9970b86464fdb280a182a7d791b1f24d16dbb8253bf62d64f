.fac = function MatGridAvatarCssMatStyler_Factory(t) { return new (t || MatGridAvatarCssMatStyler)(); };\r\nMatGridAvatarCssMatStyler.dir = /*@__PURE__*/ ngcc0.defineDirective({ type: MatGridAvatarCssMatStyler, selectors: [[\"\", \"mat-grid-avatar\", \"\"], [\"\", \"matGridAvatar\", \"\"]], hostAttrs: [1, \"mat-grid-avatar\"] });\r\n(function () { (typeof ngDevMode === \"undefined\" || ngDevMode) && ngcc0.setClassMetadata(MatGridAvatarCssMatStyler, [{\r\n        type: Directive,\r\n        args: [{\r\n                selector: '[mat-grid-avatar], [matGridAvatar]',\r\n                host: { 'class': 'mat-grid-avatar' }\r\n            }]\r\n    }], null, null); })();\r\n/**\r\n * Directive whose purpose is to add the mat- CSS styling to this selector.\r\n * @docs-private\r\n */\r\nclass MatGridTileHeaderCssMatStyler {\r\n}\r\nMatGridTileHeaderCssMatStyler.fac = function MatGridTileHeaderCssMatStyler_Factory(t) { return new (t || MatGridTileHeaderCssMatStyler)(); };\r\nMatGridTileHeaderCssMatStyler.dir = /*@__PURE__*/ ngcc0.defineDirective({ type: MatGridTileHeaderCssMatStyler, selectors: [[\"mat-grid-tile-header\"]], hostAttrs: [1, \"mat-grid-tile-header\"] });\r\n(function () { (typeof ngDevMode === \"undefined\" || ngDevMode) && ngcc0.setClassMetadata(MatGridTileHeaderCssMatStyler, [{\r\n        type: Directive,\r\n        args: [{\r\n                selector: 'mat-grid-tile-header',\r\n                host: { 'class': 'mat-grid-tile-header' }\r\n            }]\r\n    }], null, null); })();\r\n/**\r\n * Directive whose purpose is to add the mat- CSS styling to this selector.\r\n * @docs-private\r\n */\r\nclass MatGridTileFooterCssMatStyler {\r\n}\r\nMatGridTileFooterCssMatStyler.fac = function MatGridTileFooterCssMatStyler_Factory(t) { return new (t || MatGridTileFooterCssMatStyler)(); };\r\nMatGridTileFooterCssMatStyler.dir = /*@__PURE__*/ ngcc0.defineDirective({ type: MatGridTileFooterCssMatStyler, selectors: [[\"mat-grid-tile-footer\"]], hostAttrs: [1, \"mat-gri